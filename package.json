{
  "name": "blep-wk-v2",
  "version": "0.1.0",
  "bin": {
    "blep-wk-v2": "bin/blep-wk-v2.js"
  },
  "scripts": {
    "build": "tsc  && cdk synth",
    "watch": "tsc -w",
    "test:unit": "jest --coverage",
    "test:integ": "jest -c jest.integration.config.ts --passWithNoTests",
    "test:viewer": "yarn open-cli ./coverage/lcov-report/index.html",
    "test": "yarn test:unit && yarn test:integ",
    "cdk": "cdk",
    "lint": "eslint . --ignore-path .gitignore --ext .ts",
    "format": "prettier --config .prettierrc --ignore-path .gitignore --write ."
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "aws-cdk": "^1.120.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "jest": "^27.1.0",
    "jest-dynalite": "^3.4.4",
    "open-cli": "^7.0.1",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "~4.4.2"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.83",
    "aws-sdk": "^2.1009.0",
    "constructs": "^3.3.69",
    "monocdk": "^1.120.0"
  },
  "peerDependencies": {
    "constructs": "^3.3.69"
  }
}
